<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core"
	confirm=".handleClose">
	<SelectDialog
		id="mySelectDialog"
		noDataText="No Products Found"
		title="Select Product"
		search=".onSearch"
		confirm="onCloseDialogFlussi"
		cancel="onCloseDialogFlussi"
		items="{flussiModel>/flussi}">
		<StandardListItem
			title="{flussiModel>nome}"
			description=""
			icon=""
			iconDensityAware="false"
			iconInset="false"
			type="Active" />
	</SelectDialog>
</core:FragmentDefinition>